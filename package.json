{
  "name": "@getdrip/much-select-elm",
  "version": "0.6.0",
  "description": "A fancy selector web component written (mostly) in elm.",
  "main": "dist/much-select.js",
  "type": "module",
  "module": "dist/much-select.js",
  "repository": "github:DripEmail/much-select-elm",
  "engines": {
    "node": ">=12.14",
    "npm": ">=7.13.0"
  },
  "scripts": {
    "start": "./scripts/serve.sh",
    "test": "elm-test",
    "web-test-runner": "web-test-runner tests/**/*.test.html --node-resolve",
    "web-test-runner-with-coverage": "web-test-runner tests/**/*.test.html --coverage --node-resolve",
    "build": "./scripts/build.sh",
    "watch": "chokidar \"src/**/*.js\" \"src/**/*.elm\" -c \"npm run build\" --initial",
    "serve": "scripts/serve.sh",
    "build-site": "./scripts/build-site.sh",
    "deploy-site": "npm run build-site && firebase deploy",
    "lint": "eslint",
    "format": "elm-format"
  },
  "author": "Jachin Rupe <jachin.rupe@drip.com>",
  "license": "MIT",
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4",
    "@open-wc/testing": "^3.0.3",
    "@web/test-runner": "^0.13.22",
    "@web/test-runner-mocha": "^0.7.5",
    "chokidar-cli": "^3.0.0",
    "elm-esm": "^1.1.2",
    "elm-format": "^0.8.5",
    "elm-hot": "^1.1.6",
    "elm-outdated": "^1.1.1",
    "elm-test": "0.19.1-revision7",
    "elm-tooling": "^1.6.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "node-elm-compiler": "^5.0.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.9"
  }
}
